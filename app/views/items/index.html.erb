<div class="wrapper">
<h1>トップページ</h1>
<div><% if admin_signed_in? %>管理者でログインしています ID:<%= current_admin.id %>
<% else %>
ログインしていません<% end %></div>
<div>
<div><% if user_signed_in? %>ユーザーでログインしています ID:<%= current_user.id %>
<% else %>
ログインしていません<% end %></div>

  <%= link_to "管理者 新規登録", new_admin_registration_path %><br>
  <%= link_to "管理者 ログイン", new_admin_session_path %><br>
  <%= link_to "管理者 ログアウト", destroy_admin_session_path, method: :delete %><br>
  <%= link_to "出品ページ", new_item_path %><br>
  <%= link_to "カテゴリ追加", new_category_path %><br>
  <br>
  <%= link_to "ユーザー 新規登録", new_user_registration_path %><br>
  <%= link_to "ユーザー ログイン", new_user_session_path %><br>
  <%= link_to "ユーザー ログアウト", destroy_user_session_path, method: :delete %>
</div>
<% @items.each do |item| %>
  <%= link_to item_path(item.id) do %>
    <%= item.name%>
    <div class="items">
      <% item.images.first(1).each do |image| %>
        <%= image_tag image.variant(resize: '320x320'), class:'images' if item.images.attached? %>
      <% end %>
    </div>
  <% end %>
<% end %>