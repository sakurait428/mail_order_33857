<!DOCTYPE html>
<html>
  <head>
    <title>MailOrder33857</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <div><%= link_to image_tag('/assets/logo/logo.jpg'), root_path %></div>
      <li>
        <ul><%= image_tag "/assets/logo/gnav01.jpg" %></ul>
        <ul><%= link_to image_tag('/assets/logo/gnav02.jpg'), '#' %></ul>
        <ul><%= link_to image_tag('/assets/logo/gnav03.jpg'), '#' %></ul>
        <ul><%= image_tag "/assets/logo/gnav04.jpg" %></ul>
        <ul><%= link_to image_tag('/assets/logo/gnav05.jpg'), '#' %></ul>
        <ul><%= link_to image_tag('/assets/logo/gnav06.jpg'), '#' %></ul>
        <ul><%= link_to image_tag('/assets/logo/gnav07.jpg'), '#' %></ul>
      <li>
      <div>
        <% if user_signed_in? %>
          <%= link_to image_tag('/assets/logo/header-menu03.gif'), destroy_user_session_path, method: :delete, class: "btn" %>
        <% elsif admin_signed_in?%>
          <%= link_to image_tag('/assets/logo/header-menu07.gif'), destroy_admin_session_path, method: :delete, class: "btn" %>
        <% else %>
        <%= link_to image_tag('/assets/logo/header-menu01.jpg'), new_user_session_path, class: "btn" %><% end %>
        <%= link_to image_tag('/assets/logo/header-menu02.jpg'), cart_path(current_cart.id), class: "btn" %>
      </div>
    </header>
    <%= yield %>

    <div class="border"></div>
    <footer>
      <div>
        <%= link_to image_tag('/assets/logo/footer-logo.jpg'), root_path %>
      </div>
      <li class="mini">
        <ul><%= link_to "当サイトについて", '#' %></ul>
        <ul>|</ul>
        <ul><%= link_to "サイトマップ", '#' %></ul>
        <ul>|</ul>
        <ul><%= link_to "ログイン/新規会員登録", '#' %></ul>
        <ul>|</ul>
        <ul><%= link_to "特定商取引に関する法律に基づく表記", '#' %></ul>
        <ul>|</ul>
        <ul><%= link_to "お問い合わせ", '#' %></ul>
      <li>
      <div>
        <%= link_to image_tag('/assets/logo/sns-btn.jpg'), '#', class: "btn" %>
        <%= link_to image_tag('/assets/logo/sns-btn02.jpg'), '#', class: "btn" %>
        <%= link_to image_tag('/assets/logo/sns-btn03.jpg'), '#', class: "btn" %>
      </div>
    </footer>
  </body>
</html>
